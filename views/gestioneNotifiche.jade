doctype html
html(lang='en')
    head
        title Notifications Center
        meta(charset='utf-8')
        meta(name='viewport', content='width=device-width, initial-scale=1')
        link(rel='stylesheet', href='https://maxcdn.bootstrapcdn.com/font-awesome/4.6.1/css/font-awesome.min.css')
        //link(rel='stylesheet', href='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css')
        link(rel='stylesheet', href='http://ajax.aspnetcdn.com/ajax/jquery.ui/1.12.1/themes/ui-lightness/jquery-ui.css', type='text/css', media='all')
        link(rel='stylesheet', href='stylesheets/bootstrap.css')
        link(rel='stylesheet', href='stylesheets/jquery.dataTables.min.css')
        link(rel='stylesheet', href='stylesheets/buttons.bootstrap.min.css')
        script(src='https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js')
        script(src='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js')
        script(src='https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.9.0/moment-with-locales.js')
        script(src='https://cdn.rawgit.com/Eonasdan/bootstrap-datetimepicker/master/build/js/bootstrap-datetimepicker.min.js')
        script(src='javascripts/datatables.min.js')
        script(src='http://ajax.aspnetcdn.com/ajax/jquery.ui/1.12.1/jquery-ui.min.js', type='text/javascript')
        script(src='javascripts/dataTables.buttons.min.js')
        script(src='javascripts/pdfmake.min.js')
        script(src='javascripts/buttons.print.min.js')
        script(src='javascripts/buttons.flash.min.js')
        script(src='javascripts/jszip.min.js')
        script(src='javascripts/vfs_fonts.js')
        script(src='javascripts/buttons.html5.min.js')

    body(style='padding-top: 75px;')
        block myNav
        nav.navbar.navbar-default.navbar-fixed-top
            .container-fluid
                .navbar-header
                    button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#navbar', aria-expanded='false', aria-controls='navbar')
                        span.sr-only Toggle navigation
                        span.icon-bar
                        span.icon-bar
                        span.icon-bar
                    img.navbar-brand(src='images/centerLogo.svg', alt='Company Logo', width='50', height='50')
                    a.navbar-brand(href='/home') Notifications Center
                #navbar.navbar-collapse.collapse
                    ul.nav.navbar-nav
                        li.dropdown
                            a.dropdown-toggle(href='#', data-toggle='dropdown', role='button', aria-haspopup='true', aria-expanded='false')
                                | Anagrafica
                                span.caret
                            ul.dropdown-menu
                                li
                                    a(href='/aggiungiContatto') Creazione
                                li.divider(role='separator')
                                li
                                    a(href='/modificaContatto') Modifica
                        li.dropdown
                            a.dropdown-toggle(href='#', data-toggle='dropdown', role='button', aria-haspopup='true', aria-expanded='false')
                                | Eventi
                                span.caret
                            ul.dropdown-menu
                                li
                                    a(href='/creaEvento') Creazione
                                li.divider(role='separator')
                                li
                                    a(href='/modificaEvento') Modifica
                        li.dropdown
                            a.dropdown-toggle(href='#', data-toggle='dropdown', role='button', aria-haspopup='true', aria-expanded='false')
                                | Gestione
                                span.caret
                            ul.dropdown-menu
                                li
                                    a(href='/assegnaEvento') Invio Notifiche
                                li.divider(role='separator')
                                li.active
                                    a(href='/gestioneNotifiche') Gestione Report
                    ul.nav.navbar-nav.navbar-right
                        li.active
                            a(href='/logout') Logout
        block myContainer
        .container-fluid
            .jumbotron
                .row
                    h1 Gestione Report

            .row
                .col-sm-12.table-responsive
                    table#tabellaNotifiche.table.table-striped.table-bordered(width='100%', cellspacing='0')
                        thead
                            tr
                                th Id
                                th Matricola
                                th Cognome
                                th Nome
                                th Professione
                                th Titolo Evento
                                th Data Invio
                                th Tipo
                                th Stato
                                th Confermato
                                th Eliminato
                        tfoot
                            tr
                                th
                                th
                                th
                                th
                                th
                                th
                                th
                                th
                                th
                                th
                                th
            .row
                .col-sm-2
                    h3 Esporta Report:
            .row
                .col-sm-2
                   button#exportExcel.btn.btn-default.btn-block(onclick='exportExcel();') Excel
                .col-sm-2
                    button#exportPdf.btn.btn-default.btn-block(onclick='exportPdf();') PDF
                .col-sm-2
                    button#exportStampa.btn.btn-default.btn-block(onclick='exportStampa();') Stampa
    br
    br
    br
    nav.navbar.navbar-default.navbar-fixed-bottom
        .container-fluid
            br
            p.copyright.pull-right
                | Â© 2018 Notifications Center -
                a(href='http://www.ak12srl.it/')  AK-12 srl &nbsp;
                img(src='images/ak12.png', alt='Company Logo', width='40', height='20')
    block myScript
        script(src="javascripts/controllers/gestioneNotificheController.js")